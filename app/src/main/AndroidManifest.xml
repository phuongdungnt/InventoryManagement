<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.nat.inventorymanagement">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logo"
        android:label="IM"
        android:roundIcon="@drawable/ic_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.InventoryManagement">
        <activity android:name=".Adapter.GoodSellAdapterActivity" />
        <activity android:name=".HistoryAdd.DetailHistoryAddActivity" />
        <activity android:name=".HistoryAdd.HistoryAddGoodActivity" />
        <activity android:name=".ResetPass.ResetPasswordActivity" />
        <activity
            android:name=".Adapter.GoodAdapterActivity"
            tools:ignore="Instantiatable" />
        <activity android:name=".Profile.EditProfileActivity" />
        <activity android:name=".Goods.DetailGoodsActivity" />
        <activity android:name=".Goods.GoodsActivity" />
        <activity android:name=".Goods.CreateGoodsActivity" />
        <activity android:name=".Signup.SignupActivity" />
        <activity android:name=".Login.LoginActivity" />
        <activity android:name=".Login.PrivacyPolicyActivity" />
        <activity android:name=".Start.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Start.MainActivity" />
    </application>

</manifest>